<?xml version="1.0" encoding="UTF-8" ?>   
<!DOCTYPE mapper   
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 表名称：T99_CONCEPT_MODEL_DIC -->
<!-- <typeAlias type="com.idap.dataprocess.definition.entity.ConceptModelDic" alias="ConceptModelDic"/> -->
<mapper namespace="com.idap.dataprocess.definition.entity.ConceptModelDic">
	<!-- *.来映射文件字段与类属性的对就关系 ibatis默认不对字段进行去下划线工作 -->
	<resultMap id="conceptModelDicMap" type="com.idap.dataprocess.definition.entity.ConceptModelDic">
		<id property="conceptModelId" column="CONCEPT_MODEL_ID" jdbcType="VARCHAR" />
		<result property="conceptModelName" column="CONCEPT_MODEL_NAME" jdbcType="VARCHAR" />
		<result property="conceptModelDesc" column="CONCEPT_MODEL_DESC" jdbcType="VARCHAR" />
		<result property="dataType" column="DATA_TYPE" jdbcType="VARCHAR" />
		<result property="length" column="LENGTH" jdbcType="NUMERIC" />
	</resultMap>

	<!--================= 动态内容部分 ================= -->
	<!-- where：动态生成部分 -->
	<sql id="dynamicWhere">
		<!-- LIKE 样例： SYSTEM_NAME like '%'||#{systemName}||'%' -->
		<trim prefix="WHERE" prefixOverrides="AND|OR">
			<if test="conceptModelId!=null">
				AND CONCEPT_MODEL_ID=#{conceptModelId}
			</if>
			<if test="conceptModelName!=null">
				AND CONCEPT_MODEL_NAME=#{conceptModelName}
			</if>
			<if test="conceptModelDesc!=null">
				AND CONCEPT_MODEL_DESC=#{conceptModelDesc}
			</if>
			<if test="dataType!=null">
				AND DATA_TYPE=#{dataType}
			</if>
			<if test="length!=null">
				AND LENGTH=#{length}
			</if>
			<if test="sno!=null and eno!=null and orderBy==null">
				AND ROWNUM <![CDATA[<=]]>${eno}
			</if>
		</trim>

	</sql>

	<!-- order by 排序 -->
	<sql id="dynamicOrderBy">
		<if test="orderBy!=null">
			order by ${orderBy}
			<if test="order!=null">
				${order}
			</if>
		</if>
	</sql>

	<!-- group by 分组 -->
	<sql id="dynamicGroupBy">
		<if test="groupBy!=null">
			group by ${groupBy}
		</if>
	</sql>

	<!-- 表所有的字段：insert 的表、字段 部分 -->
	<sql id="allColumn">
		CONCEPT_MODEL_ID,
		CONCEPT_MODEL_NAME,
		CONCEPT_MODEL_DESC,
		DATA_TYPE,
		LENGTH
	</sql>

	<!-- oracle 查询有三种情况：1.单表直接查询 2.单表无排序分页查询 3.单表排序分页查询 -->
	<sql id="queryBody">
		<!-- 有orderby -->
		<if test="sno!=null and eno!=null and orderBy!=null">
			select
			<include refid="allColumn" />
			from
			(
			select orderContentTab.*,ROWNUM AS rowno
			from
			(
		</if>
		<!-- 无orderby -->
		<if test="sno!=null and eno!=null and orderBy==null">
			select
			<include refid="allColumn" />
			from
			(
		</if>
		<!-- 查询的主体内容：start -->
		select
		<!-- 无orderby -->
		<if test="sno!=null and eno!=null and orderBy==null">
			ROWNUM AS rowno,
		</if>
		<include refid="allColumn" />
		from
		T99_CONCEPT_MODEL_DIC
		<include refid="dynamicWhere" />
		<include refid="dynamicOrderBy" />
		<!-- 查询的主体内容：end -->
		<!-- 无orderBy -->
		<if test="sno!=null and eno!=null and orderBy==null">
			) pageTab
			WHERE pageTab.rowno <![CDATA[>=]]>
			${sno}
		</if>
		<!-- 有orderBy -->
		<if test="sno!=null and eno!=null and orderBy!=null">
			) orderContentTab
			WHERE ROWNUM <![CDATA[<=]]>${eno}
			) pageTab
			WHERE pageTab.rowno <![CDATA[>=]]>${sno}
		</if>
	</sql>


	<!-- ================================================= -->
	<!-- 1.1根据id查询对像 -->
	<select id="getById" parameterType="String" resultMap="conceptModelDicMap">
		select
		<include refid="allColumn" />
		from T99_CONCEPT_MODEL_DIC where id=#{value}
	</select>

	<!-- 2.1.根据条件查询 列表 分页 排序 -->
	<select id="findByPager" parameterType="java.util.Map" resultMap="conceptModelDicMap">
		<include refid="queryBody" />
	</select>
	<select id="query" parameterType="java.util.Map" resultMap="conceptModelDicMap">
		<include refid="queryBody" />
	</select>

	<!-- 3.1.根据对像条件查询记录数 MAP参数 -->
	<select id="findByPagerCount" parameterType="java.util.Map" resultType="int">
		select
		count(1)
		from
		T99_CONCEPT_MODEL_DIC
		<include refid="dynamicWhere" />
	</select>



	<!-- =========================自定义操作======================== -->

</mapper>




